<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.heartinfei.testsample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".target.NetActivity"
            android:label="@string/net_activity" />
        <activity
            android:name=".target.IntentTestActivity"
            android:label="@string/intent_activity" />
        <activity
            android:name=".target.FragmentTestActivity"
            android:label="@string/fragment_test" />
        <activity
            android:name=".target.MultiWindowTestActivity"
            android:label="@string/multi_window" />
        <activity android:name=".target.HelloWorldActivity"></activity>
    </application>

</manifest>